{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/onetv.m3u", "dest": "/api/onetv.js" }
  ],
  "functions": {
    "api/aupl.js": {
      "runtime": "nodejs18.x"
    }
  },
  "cron": {
    "jobs": [
      {
        "path": "/api/aupl",
        "schedule": "*/5 * * * *"
      }
    ]
  }
}
