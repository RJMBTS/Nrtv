{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/onetv\\.m3u$",
      "dest": "/api/onetv"
    },
    {
      "src": "/(.*)\\.m3u8$",
      "dest": "/api/channel?channel=$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
